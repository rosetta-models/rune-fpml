    eventsOrInfoModel EventsOrInfoModel (0..1)
    creditLimitInformation fpml.consolidated.business.events.CreditLimitInformation (0..*)
    partiesAndAccountsModel fpml.consolidated.shared.PartiesAndAccountsModel (0..1)

type ConsentRefused extends fpml.consolidated.msg.ResponseMessage: <"A message indicating that the sender does not grant consent for the recipient to perform the requested action.">
    [rootType]

    ^type fpml.consolidated.doc.ApprovalType (0..1) <"The type of approval (e.g. \"pre-clearing credit\").">
