type RequestConfirmation extends fpml.consolidated.msg.CorrectableRequestMessage: <"The confirmation process starts with the requestConfirmation message. The message may be used to request the confirmation of a new trade or any other event supported by FpML such as novation, terminations, amendments, etc. A message type defining the start of the confirmation process. The message may be used to request the confirmation of a new trade or any other event supported by FpML such as novation, terminations, amendments, etc.">
    [rootType]

    tradingAndPostTradeEventsModel fpml.consolidated.business.events.TradingAndPostTradeEventsModel (0..1)
    optionExercise fpml.consolidated.business.events.OptionExercise (0..1) <"A structure describing an option exercise event. The optionExercise event supports partial exercise (specify the number of options or amount to exercise), full exercise (use fullExercise flag), as well as the option to request options not to be exercised.">
    partiesAndAccountsModel fpml.consolidated.shared.PartiesAndAccountsModel (1..1)

type RequestConfirmationRetracted extends ConfirmationRetracted: <"A requestConfirmation message may be cancelled using the requestConfirmationRetracted message.">
    [rootType]

type ConfirmationAcknowledgement extends EventRequestAcknowledgement: <"A business acknowledgement message to indicate that the previously sent message was sucessfully processed.">
    [rootType]

type ConfirmationException extends fpml.consolidated.msg.Exception: <"A message sent to inform another system that some exception has been detected.">
    [rootType]

type ConfirmationStatus extends fpml.consolidated.msg.ResponseMessage: <"The confirmationStatus message provides the status of the matching process: matched, mismatched, unmatched, or alleged. It may also provide the best fit trade(s) or event(s) as result of the matching process. Message for sending matching results. Response message that returns the status of an event that have been submitted for matching.">
    [rootType]

    status fpml.consolidated.msg.EventStatus (1..1) <"Defines the confirmation status of a trade or post-trade event (e.g. Matched, Mismatched, Unmatched, Alleged).">
    confirmationStatusSequence ConfirmationStatusSequence (0..1)
    allegedEvent fpml.consolidated.business.events.EventsChoice (0..1) <"Event (trade post-trade event) asserted by the \"other side\'s\" party.">
    partiesAndAccountsModel fpml.consolidated.shared.PartiesAndAccountsModel (0..1)

type ConfirmationAgreed extends fpml.consolidated.msg.ResponseMessage: <"The confirmationAgreed message is sent when the matching process returns a proposed match (trade or event) and the Confirmation Requester agrees with it. A message indicating that a confirmation has been agreed by a counterparty.">
    [rootType]

    tradingEventsBaseModel fpml.consolidated.business.events.TradingEventsBaseModel (0..1)
    postTradeEventsBaseModel fpml.consolidated.business.events.PostTradeEventsBaseModel (0..1)
    changeEventsBaseModel fpml.consolidated.business.events.ChangeEventsBaseModel (0..1)
    optionsEventsBaseModel fpml.consolidated.business.events.OptionsEventsBaseModel (0..1)
    additionalEvent fpml.consolidated.business.events.AdditionalEvent (0..1)
    partiesAndAccountsModel fpml.consolidated.shared.PartiesAndAccountsModel (0..1)

type ConfirmationDisputed extends fpml.consolidated.msg.ResponseMessage: <"The confirmationDisputed message is sent when the matching process returns a proposed match (trade or event) and the Confirmation Requester disputes it. A message indicating that a confirmation has not been agreed by a counterparty.">
    [rootType]

    tradingEventsBaseModel fpml.consolidated.business.events.TradingEventsBaseModel (0..1)
