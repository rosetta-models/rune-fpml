    value fpml.consolidated.shared.NonEmptyScheme (1..1)
    id string (0..1)
    portfolioNameScheme fpml.consolidated.shared.NonEmptyURI (0..1)

type PricingContext: <"The reason a trade\'s price does not reflect the current market price.">

    value fpml.consolidated.shared.Scheme (1..1)
    pricingContextScheme fpml.consolidated.shared.NonEmptyURI (0..1)

