type TradeDifference: <"A type used to record the details of a difference between two business objects/">

    differenceType fpml.consolidated.fpmlenum.DifferenceTypeEnum (1..1) <"The type of difference that exists.">
    differenceSeverity fpml.consolidated.fpmlenum.DifferenceSeverityEnum (1..1) <"An indication of the severity of the difference.">
    element fpml.consolidated.shared.NonEmptyToken (1..1) <"The name of the element affected.">
    basePath fpml.consolidated.shared.NonEmptyToken (0..1) <"XPath to the element in the base object.">
    baseValue fpml.consolidated.shared.NonEmptyToken (0..1) <"The value of the element in the base object.">
    otherPath fpml.consolidated.shared.NonEmptyToken (0..1) <"XPath to the element in the other object.">
    otherValue fpml.consolidated.shared.NonEmptyToken (0..1) <"Value of the element in the other trade.">
    missingElement fpml.consolidated.shared.NonEmptyToken (0..*) <"Element(s) that are missing in the other trade.">
    extraElement fpml.consolidated.shared.NonEmptyToken (0..*) <"Element(s) that are extraneous in the other object.">
    message fpml.consolidated.shared.NonEmptyNormalizedString (1..1) <"A human readable description of the problem.">

type Trader:

    value fpml.consolidated.shared.Scheme (1..1)
    traderScheme fpml.consolidated.shared.NonEmptyURI (0..1)

type VersionedContractId: <"Contract Id with Version Support">

    contractId ContractId (1..1)
    versionHistoryModel fpml.consolidated.shared.VersionHistoryModel (1..1)
